OUTPUT_FORMAT("coff-sparc")
ENTRY (_start)
SECTIONS
{
  .text  SIZEOF_HEADERS : {
    *(.init)
    *(.text)
     etext  =  .;
     ___CTOR_LIST__ = .;
     LONG((___CTOR_END__ - ___CTOR_LIST__) / 4 - 2)
     *(.ctors)
     LONG(0)
     ___CTOR_END__ = .;
     ___DTOR_LIST__ = .;
     LONG((___DTOR_END__ - ___DTOR_LIST__) / 4 - 2)
     *(.dtors)
     LONG(0)
     ___DTOR_END__ = .;
    *(.fini)
     etext  =  .;
  }
  .data  0x400000 + (. & 0xffc00fff) : {
    *(.data)
     edata  =  .;
  }
  .bss  SIZEOF(.data) + ADDR(.data) :
  {
    *(.bss)
    *(COMMON)
     end = .;
  }
  .stab  0 (NOLOAD) :
  {
    [ .stab ]
  }
  .stabstr  0 (NOLOAD) :
  {
    [ .stabstr ]
  }
}
